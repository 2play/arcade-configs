<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<item label="Wahcade">
			<action name="Execute">
				<execute>wahcade</execute>
			</action>
		</item>
		<item label="Wahcade Setup">
			<action name="Execute">
				<execute>wahcade-setup</execute>
			</action>
		</item>
        <item label="RetroArch">
            <action name="Execute">
                <execute>retroarch</execute>
            </action>
        </item>
		<item label="Terminal emulator">
			<action name="Execute">
				<execute>urxvt</execute>
			</action>
		</item>
		<item label="Chromium">
			<action name="Execute">
				<execute>chromium</execute>
			</action>
		</item>
		<separator/>
		<item label="Exit">
			<action name="Exit"/>
		</item>
	</menu>
</openbox_menu>
